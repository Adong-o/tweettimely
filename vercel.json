{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/callback", "dest": "/callback.html" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/auth", "dest": "/auth.html" },
    { "src": "/verify-email", "dest": "/verify-email.html" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/privacy-policy", "dest": "/privacy-policy.html" },
    { "src": "/terms-of-service", "dest": "/terms-of-service.html" },
    { "src": "/cookie-policy", "dest": "/cookie-policy.html" },
    { "src": "/(.*)", "dest": "/404.html", "status": 404 }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 